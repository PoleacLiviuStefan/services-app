{
  "name": "site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "start-cron": "ts-node --esm scripts/ping-cron.ts",
    "build-cron": "tsc --project tsconfig.json --outDir .cron-dist",
    "start": "next start",
    "lint": "next lint",
    "worker:test": "node src/scripts/worker-test.js",
    "worker:dev": "tsx src/scripts/start-reminder-worker.ts",
    "worker:start": "npx ts-node --esm src/scripts/start-reminder-worker.ts",
    "worker:build": "tsc src/scripts/start-reminder-worker.ts --outDir dist",
    "worker:prod": "node dist/start-reminder-worker.js",
    "migrate:sessions": "ts-node src/scripts/migrate-existing-sessions.ts",
    "migrate:single": "ts-node src/scripts/migrate-existing-sessions.ts",
    "queue:status": "curl -X GET http://localhost:3000/api/admin/queue",
    "queue:clean": "curl -X POST http://localhost:3000/api/admin/queue -H 'Content-Type: application/json' -d '{\"action\": \"clean_queue\"}'",
    "queue:start-worker": "curl -X POST http://localhost:3000/api/admin/queue -H 'Content-Type: application/json' -d '{\"action\": \"start_worker\"}'",
    "queue:stop-worker": "curl -X POST http://localhost:3000/api/admin/queue -H 'Content-Type: application/json' -d '{\"action\": \"stop_worker\"}'",
    "test:redis": "node scripts/test-redis.js",
    "test:mail": "ts-node -e \"import('./lib/mail').then(({sendConsultationReminder24h}) => sendConsultationReminder24h('test@example.com', 'Test User', 'Test Provider', new Date(Date.now() + 24*60*60*1000).toISOString(), new Date(Date.now() + 25*60*60*1000).toISOString()).then(() => console.log('âœ… Email sent')))\"",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:seed": "ts-node prisma/seed.ts",
    "db:studio": "prisma studio",
    "docker:build": "docker build -t mysticgold-worker .",
    "docker:run": "docker run -d --name mysticgold-worker --env-file .env mysticgold-worker",
    "docker:stop": "docker stop mysticgold-worker && docker rm mysticgold-worker",
    "pm2:start": "pm2 start scripts/start-reminder-worker.ts --name consultation-reminders --interpreter ts-node",
    "pm2:stop": "pm2 stop consultation-reminders",
    "pm2:restart": "pm2 restart consultation-reminders",
    "pm2:logs": "pm2 logs consultation-reminders",
    "pm2:monitor": "pm2 monit",
    "heroku:worker": "npm run worker:start",
    "railway:worker": "npm run worker:start"
  },
